{
  "manifest_version": 2,
  "name": "Website Blocker",
  "version": "1.0",
  "description": "Tracks new tabs/websites and blocks non-whitelisted sites.",
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "alarms",
    "storage",
    "<all_urls>"
  ],

  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html"
  }
}
  