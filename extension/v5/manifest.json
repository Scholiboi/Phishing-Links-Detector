{
    "manifest_version": 3,
    "name": "Website Sandbox Viewer",
    "version": "1.0",
    "permissions": ["activeTab", "storage"],
    "host_permissions": ["<all_urls>"],
    "action": {
        "default_popup": "popup.html"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self';"
    }
}
