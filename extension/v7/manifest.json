{
  "manifest_version": 3,
  "name": "Website Blocker",
  "version": "1.0",
  "description": "Tracks new tabs/websites and blocks non-whitelisted sites.",
  "permissions": [
    "tabs",
    "scripting",
    "declarativeNetRequest"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
  